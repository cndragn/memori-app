<%= render 'layouts/imageheader' %>

<p id="notice"><%= notice %></p>

<p>
  <strong>Label:</strong>
  <%= @category.label %>
</p>

<h2>Decks</h2>
<ul>
  <% @category.decks.each do |adeck| %>
    <li><%= link_to adeck.title, adeck %>
  <% end %>
</ul>

<p>
<%= link_to 'New Deck', new_category_deck_path(@category) %>
</p>

<%= link_to 'Edit', edit_category_path(@category) %> |
<%= link_to 'Back', categories_path %>
